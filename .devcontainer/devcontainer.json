{
    "name": "Datadog-Agent-DevEnv",
    "runArgs": [
        "--cap-add=SYS_PTRACE",
        "--security-opt",
        "seccomp=unconfined"
    ],
    "remoteUser": "datadog",
    "customizations": {
        "vscode": {
            "settings": {
                "go.toolsManagement.checkForUpdates": "local",
                "go.useLanguageServer": true,
                "go.gopath": "/home/datadog/go",
                "go.goroot": "/usr/local/go",
                "go.buildTags": "otlp,consul,ec2,jmx,secrets,gce,apm,process,python,zk,kubelet,kubeapiserver,zlib,etcd,orchestrator",
                "go.testTags": "otlp,consul,ec2,jmx,secrets,gce,apm,process,python,zk,kubelet,kubeapiserver,zlib,etcd,orchestrator"
            },
            "extensions": [
                "golang.Go"
            ]
        }
    },
    "image": "486234852809.dkr.ecr.us-east-1.amazonaws.com/ci/datadog-agent-devenv:1"
}