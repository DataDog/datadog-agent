# We need an unreleased recent commit because we need support for local path replaces
# https://github.com/bazelbuild/bazel-gazelle/pull/1776
git_override(
    module_name = "gazelle",
    commit = "154113369b50970d1699397b5dd24d2fca088dda",
    remote = "https://github.com/bazelbuild/bazel-gazelle",
)

go_sdk = use_extension("@rules_go//go:extensions.bzl", "go_sdk")
go_sdk.download(version = "1.21.9")

go_deps = use_extension("@gazelle//:extensions.bzl", "go_deps")
go_deps.from_file(go_work = "//:go.work")

# grpc-gateway comes with BUILD files of its own, but I have no clue how to make use of that
# This is my best attempt so far at making this dependency work
go_deps.gazelle_override(
    directives = [
        "gazelle:go_naming_convention import",
        "gazelle:prefix github.com/grpc-ecosystem/grpc-gateway",
        "gazelle:exclude third_party",
        "gazelle:exclude vendor",
        "gazelle:exclude _output",
    ],
    path = "github.com/grpc-ecosystem/grpc-gateway",
)

