go_deps = use_extension("@gazelle//:extensions.bzl", "go_deps")
go_deps.from_file(go_work = "//:go.work")
go_deps.gazelle_default_attributes(
    directives = [
        # Generally speaking we don't need to generate code from protos from 3rd party repos,
        # and they trip up rules_go, so we disable them by default
        "gazelle:proto disable",
        # Resolve dependencies that depend back on this same repo (cyclical deps) to references to this repo
        "gazelle:resolve go github.com/DataDog/datadog-agent/pkg/proto/pbgo/trace @@//pkg/proto/pbgo/trace",
        "gazelle:resolve go github.com/DataDog/datadog-agent/pkg/obfuscate @@//pkg/obfuscate",
        "gazelle:resolve go github.com/DataDog/datadog-agent/pkg/remoteconfig/state @@//pkg/remoteconfig/state",
        "gazelle:resolve go github.com/DataDog/datadog-agent/pkg/trace/stats @@//pkg/trace/stats",
        "gazelle:resolve go github.com/DataDog/datadog-agent/pkg/util/hostname/validate @@//pkg/util/hostname/validate",
        "gazelle:resolve go github.com/DataDog/datadog-agent/pkg/trace/log @@//pkg/trace/log",
    ],
)

# go-systemd requires systemd headers to build, this patch adds them as a dependency
go_deps.module_override(
    patches = [
        "bazel/patch/go_systemd_systemd_headers.patch",
    ],
    path = "github.com/coreos/go-systemd",
)

# Godror uses CGO, that alone requires some patching to the autogenerated BUILD.bazel file (to include the
# necessary C files).
# But to make matters worse, it references .c files in include directives.
# Go with CGO always compiles every .c file it sees, which results in those files being compiled twice,
# resulting in linking errors.
# The solution was to rename all those .c files to .h files and fix the necessary includes to make everything wor.
go_deps.module_override(
    patches = [
        "bazel/patch/godror-bazel.patch",
    ],
    path = "github.com/godror/godror",
)
