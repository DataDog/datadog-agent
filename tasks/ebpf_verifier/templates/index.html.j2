<html>
	<head>
		<title>{{ title }}</title>
	</head>
	<body>
		<h1>{{ title }}</h1>

		<table>
			<tr>
				<th>Program</th>
				<th>Instructions</th>
				<th>Stack usage</th>
				<th>Max states per insn.</th>
				<th>Total states</th>
				<th>Peak state</th>
			</tr>
			{% for object_file, objstats in verifier_stats.items() %}
				<tr>
					<td colspan="6">{{ object_file }}</td>
				</tr>
				{% for function, objstat in objstats.items() %}
					<tr>
						<td>
							<a href="{{ object_file }}/{{ function }}.html">
								{{ function }}
							</</td>
						<td>{{ objstat.instruction_processed }}</td>
						<td>{{ objstat.stack_usage }}</td>
						<td>{{ objstat.max_states_per_insn }}</td>
						<td>{{ objstat.total_states }}</td>
						<td>{{ objstat.peak_states }}</td>
					</tr>
				{% endfor %}
			{% endfor %}
	</body>
</html>
