{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "dns.json",
    "type": "object",
    "allOf": [
        {
            "$ref": "/schemas/event.json"
        },
        {
            "$ref": "/schemas/usr.json"
        },
        {
            "$ref": "/schemas/process_context.json"
        },
        {
            "date": {
                "$ref": "/schemas/datetime.json"
            }
        },
        {
            "properties": {
                "dns": {
                    "type": "object",
                    "required": [
                        "id",
                        "qdcount",
                        "qclass",
                        "qtype",
                        "dns_server_ip",
                        "name"
                    ],
                    "properties": {
                        "id": {
                            "type": "integer"
                        },
                        "qdcount": {
                            "type": "integer"
                        },
                        "qclass":{
                            "type": "string"
                        },
                        "qtype": {
                            "type": "string"
                        },
                        "dns_server_ip": {
                            "type": "string"
                        },
                        "name": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    ]
}
