{
    "family": "dd-agent-registry-test",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
        {
            "name": "datadog-agent",
            "image": "datadog/agent-dev:update-registry-not-atomic-py3-jmx",
            "essential": true,
            "environment": [
                {
                    "name": "DD_API_KEY",
                    "value": "wacktest"
                },
                {
                    "name": "ECS_FARGATE",
                    "value": "true"
                },
                {
                    "name": "DD_HOSTNAME",
                    "value": "registry-test-fargate"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/dd-agent-registry-test",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs",
                    "awslogs-create-group": "true"
                }
            }
        }
    ],
    "executionRoleArn": "arn:aws:iam::aws:policy/service-role/AmazonECSTaskExecutionRolePolicy"
} 