<%= JSON.parse(({'logs' => @logs }).to_json).to_yaml %>

instances:
  <% @instances.each do |i| -%>
  - lighttpd_status_url: <%= i['status_url'] %>
    <% if i.key?('tags') -%>
    tags:
      <% i['tags'].each_pair do |k, v| -%>
      - <%= k %>: <%= v %>
      <% end -%>
    <% end -%>
  <% end -%>

init_config:
# No init_config section needed
